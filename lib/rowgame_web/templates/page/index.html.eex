<h1>Row game Login</h1>
<%= form_tag("/session", method: :post) do %>
  <div class="form-group">
    Username:
    <input type="text" name="username">
  </div>
  <div class="form-group">
    <button class="btn btn-primary">Log in</button><br/>
    <%= link "Register", to: user_path(@conn, :new) %>
  </div>
<% end %>
